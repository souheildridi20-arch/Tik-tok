        }, 4500);
    }

    // ØªØ­Ø¯ÙŠØ« Ø¹Ø¯Ø¯ Ø§Ù„Ù…Ø³ØªØ®Ø¯Ù…ÙŠÙ† Ø§Ù„ÙˆÙ‡Ù…ÙŠ
    setInterval(() => {
