            addTerminalLine('Ø§Ù„Ø§ØªØµØ§Ù„ Ø¨Ø§Ù„Ø®ÙˆØ§Ø¯Ù… Ø§Ù„ÙˆÙ‡Ù…ÙŠØ©...', 'CONNECT');
        }, 500);

        setTimeout(() => {
            addTerminalLine('Ø§Ù„ØªØ­Ù‚Ù‚ Ù…Ù† Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª...', 'VERIFY');
        }, 1500);

        setTimeout(() => {
            addTerminalLine('Ù…Ø¹Ø§Ù„Ø¬Ø© Ø§Ù„Ø·Ù„Ø¨...', 'PROCESS');
        }, 2500);

        setTimeout(() => {
            const random = Math.floor(Math.random() * 1000);
            addTerminalLine(`ØªÙ… Ø¥Ø¶Ø§ÙØ© ${random} ${service} Ø¨Ù†Ø¬Ø§Ø­ (ÙˆÙ‡Ù…ÙŠ)`, 'SUCCESS');
